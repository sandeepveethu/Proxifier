{
	"manifest_version": 2,
	"name": "Proxifier",
	"version": "1.0",
	"description": "Obtains working proxies from \"http://172.16.114.121/PROXY_CHECKER/\".Click to set the proxy.",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"browser_action": {
		"default_icon": "icons/icon16.png",
		"default_title": "Proxifier",
		"default_popup": "popup.html"
	},
	"permissions": ["*://*/*","proxy"]
}